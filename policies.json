{
    "__COMMENT__ More Information": "https://github.com/mozilla/policy-templates/blob/master/README.md",
    "policies": {
        "AppUpdateURL": "https://0.0.0.0",
        "DisableAppUpdate": true,
        "OverrideFirstRunPage": "",
	"OverridePostUpdatePage": "",
        "DisableSystemAddonUpdate": true,
        "DisableProfileImport": false,
	"DisableFirefoxAccounts": true,
        "DisableFirefoxStudies": true,
        "DisableTelemetry": true,
	"DontCheckDefaultBrowser": true,
	"EncryptedMediaExtensions": {
      	    "Enabled": false,
      	    "Locked": true
      	},
      	"OfferToSaveLogins": false,
      	"PasswordManagerEnabled": false,
      	"ShowHomeButton": true,
      	"SSLVersionMin": "tls1.2",
      	"UserMessaging": {
      	    "WhatsNew": false,
      	    "ExtensionRecommendations": false,
      	    "FeatureRecommendations": false,
      	    "UrlbarInterventions": false,
      	    "SkipOnboarding": false,
      	    "MoreFromMozilla": false
    	},
    	"Permissions": {
      		"Camera": {
        		"Allow": ["https://teams.microsoft.com"],
        		"BlockNewRequests": true,
        		"Locked": true
     		},
      		"Microphone": {
        		"Allow": ["https://teams.microsoft.com"],
        		"Block": ["https://teams.microsoft.com"],
        		"BlockNewRequests": true,
        		"Locked": true
      		},
      		"Location": {
        		"BlockNewRequests": true,
        		"Locked": true
      		},
      		"Notifications": {
        		"BlockNewRequests": true,
        		"Locked": true
      		},
     		"Autoplay": {
        		"Default": "block-audio-video",
        		"Locked": true
      		}
    	},
    	"PopupBlocking": {
      		"Default": true,
      		"Locked": true
    	},
    	"DisableFirefoxScreenshots": true,
      	"DisableBuiltinPDFViewer": true,
        "DisableFeedbackCommands": true,
        "DisablePocket": true,
        "DisableFormHistory": true,
        "DisableMasterPasswordCreation": true,
        "EnableTrackingProtection": {
      	    "Value": true,
      	    "Locked": false,
      	    "Cryptomining": true,
      	    "Fingerprinting": true,
      	    "Exceptions": ["https://127.0.0.1", "http://127.0.0.1", "https://speedport.ip", "http://speedport.ip", "http://192.168.2.1", "https://192.168.2.1"]
    	},
        "DisableSetDesktopBackground": false,
        "DisableDeveloperTools": false,
	"NetworkPrediction": false,
	"PromptForDownloadLocation": true,
	"DNSOverHTTPS": {
            "Enabled": false,
            "ProviderURL": "",
            "Locked": false
        },
	"Preferences": {
		 "dom.security.https_only_mode": {
			 "Value": true,
			 "Status": "locked"
		 },
		 "toolkit.legacyUserProfileCustomizations.stylesheets": {
			 "Value": true,
			 "Status": "locked"
		 }
	},
        "NoDefaultBookmarks": true,
        "WebsiteFilter": {
            "Block": [
                "https://localhost/*"
            ],
            "Exceptions": [
                "https://localhost/*"
            ]
        },
        "ExtensionSettings": {
            "uBlock0@raymondhill.net": {
                "installation_mode": "force_installed",
                "install_url": "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi"
                },  
            "google@search.mozilla.org": {
                "installation_mode": "blocked"
                },
            "bing@search.mozilla.org": {
                "installation_mode": "blocked"
                },
            "amazon@search.mozilla.org": {
                "installation_mode": "blocked"
                },
            "ebay@search.mozilla.org": {
                "installation_mode": "blocked"
                },
            "twitter@search.mozilla.org": {
                "installation_mode": "blocked"
                }
        }
    }
}
